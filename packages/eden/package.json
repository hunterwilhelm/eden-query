{
  "name": "@ap0nia/eden",
  "version": "1.0.11",
  "description": "Fully type-safe Elysia.js client",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ap0nia/eden-query",
    "directory": "packages/eden"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "node": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*",
      "import": "./src/*",
      "node": "./src/*",
      "default": "./src/*"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "./*": [
        "./src/*"
      ]
    }
  },
  "dependencies": {
    "elysia": "1.1.13"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "msw": "^2.4.4",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "elysia": "1.1.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
